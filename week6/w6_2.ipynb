    "def star(n):\n",
    "    print(\"\")\n",
    "    if n==1:\n",
    "        return \"*\"\n",
    "    else:\n",
    "        for i in range(1,n+1):\n",
    "            print(\"*\",end=\"\")\n",
    "        return star(n-1)\n",
    "    \n",
    "star(5)"
   ]
